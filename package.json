{
  "name": "@davicotico/iconpicker",
  "version": "1.1.0",
  "description": "Vanilla Javascript Icon Picker (made with Typescript)",
  "main": "./lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.umd.js"
    },
    "./iconsets/": {
      "import": "./lib/iconsets/",
      "require": "./lib/iconsets/"
    },
    "./styles.css": "./lib/css/styles.css",
    "./light.css": "./lib/css/themes/light.css",
    "./dark.css": "./lib/css/themes/dark.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davicotico/iconpicker.git"
  },
  "author": {
    "name": "David Tomas Ticona Saravia",
    "url": "https://davidticona.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davicotico/iconpicker/issues"
  },
  "keywords": [
    "javascript-icon-picker",
    "javascript-iconpicker",
    "vanilla-iconpicker",
    "icon-picker",
    "iconpicker",
    "icons",
    "icon-font",
    "javascript",
    "typescript",
    "tippyjs"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --config vite.config-prod.ts",
    "build:page": "tsc && vite build --config vite-config-page.ts",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.3.0"
  },
  "dependencies": {
    "tippy.js": "^6.3.7"
  }
}
